def setup():
    size(500,500)
    smooth()
    noStroke()

def draw():
    background(255)
    border=50                            #отступ от полотна
    nw=width-2*border                    #ширина таблицы
    nh=height-2*border                   #высота таблицы
    number=5                             #кол-во ячеек таблицы
    nWstep=nw/number                     #ширина ячейки
    nHstep=nh/number                     #высота ячейки
    for i in range(number):              #цикл который будет помогать строить нам кружочки и отвеающий за переменную i(массив numbr)
        for j in range(number):          #второй цикл который будет помогать строить нам кружочки и отвеающий за переменную j(массив numbr)
            x=border+i*nWstep+nWstep/2   #координата абциссы центра кружочков зависящая от i
            y=border+j*nHstep+nHstep/2   #координата ординаты центра кружочков зависящая от j
            size=5+(4-j+4-i)*10          #размер кружочков
            mColor=size*1.5              #одна из состовляющих цвета
            fill(50,20,mColor)           #мне захотелось окрасить их в такой цвет
            ellipse(x,y,size,size)       #функцмя создающая наши кружочки
            fill(250)                    #цвет точек в центре кружочков
            ellipse(x,y,3,3)             #сами точки
                                         #хочу дополнительные баллы за комментарии(ну пожалуйста :3)
